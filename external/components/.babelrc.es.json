{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-typescript",
        [
            "@babel/preset-env",
            {
                "modules": false,
                "exclude": ["@babel/plugin-transform-regenerator"]
            }
        ],
        "minify"
    ],
    "plugins": [
        [
            "babel-plugin-styled-components",
            {
                "displayName": true,
                "fileName": false
            }
        ],
        "@babel/plugin-proposal-nullish-coalescing-operator"
    ],
    "comments": false
}
